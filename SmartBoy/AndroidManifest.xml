<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.solt.jdc.smartboy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:name="com.solt.jdc.smartboy.util.SmartBoyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light" >
        <activity
            android:name="com.solt.jdc.smartboy.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.solt.jdc.smartboy.TabelActivity"
            android:icon="@drawable/ic_launcher_table"
            android:label="@string/title_activity_tabel"
            android:parentActivityName="com.solt.jdc.smartboy.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solt.jdc.smartboy.MainActivity" />
        </activity>
        <activity
            android:name="com.solt.jdc.smartboy.NewOrderActivity"
            android:icon="@drawable/ic_action_list"
            android:label="@string/title_activity_new_order"
            android:parentActivityName="com.solt.jdc.smartboy.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.solt.jdc.smartboy.MainActivity" />
        </activity>
    </application>

</manifest>
