CREATE TABLE DIVISION (ID INTEGER NOT NULL, NAME VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE TOWNSHIPS (ID INTEGER NOT NULL, NAME VARCHAR(255), DIVISION_ID INTEGER, PRIMARY KEY (ID))
CREATE TABLE ADDRESS (ID INTEGER NOT NULL, ADDRESS VARCHAR(255), TOWNSHIPS_ID INTEGER, PRIMARY KEY (ID))
CREATE TABLE AUSER (ID INTEGER NOT NULL, EMAIL VARCHAR(255), MEMO VARCHAR(255), NAME VARCHAR(255), PHONE VARCHAR(255), address_id INTEGER, PRIMARY KEY (ID))
ALTER TABLE TOWNSHIPS ADD CONSTRAINT TWNSHIPSDIVISIONID FOREIGN KEY (DIVISION_ID) REFERENCES DIVISION (ID)
ALTER TABLE ADDRESS ADD CONSTRAINT ADDRESSTOWNSHIPSID FOREIGN KEY (TOWNSHIPS_ID) REFERENCES TOWNSHIPS (ID)
ALTER TABLE AUSER ADD CONSTRAINT AUSER_address_id FOREIGN KEY (address_id) REFERENCES ADDRESS (ID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(15), PRIMARY KEY (SEQ_NAME))
