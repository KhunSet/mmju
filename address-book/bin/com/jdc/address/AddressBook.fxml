<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="545.0" prefWidth="760.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.jdc.address.AddressBook">
  <!-- TODO Add Nodes -->
  <children>
    <Label layoutX="16.0" layoutY="14.0" text="My First Address Book">
      <effect>
        <Glow>
          <input>
            <Reflection />
          </input>
        </Glow>
      </effect>
      <font>
        <Font size="25.0" />
      </font>
    </Label>
    <Label layoutX="16.0" layoutY="104.0" text="Name" />
    <TextField fx:id="name" layoutX="109.0" layoutY="101.0" prefWidth="200.0" />
    <Label layoutX="16.0" layoutY="136.0" text="Phone" />
    <TextField fx:id="phone" layoutX="109.0" layoutY="133.0" prefWidth="200.0" />
    <Label layoutX="16.0" layoutY="167.0" text="Address" />
    <TextField fx:id="address" layoutX="109.0" layoutY="164.0" prefWidth="200.0" />
    <Button layoutX="323.0" layoutY="162.0" mnemonicParsing="false" onAction="#save" prefWidth="56.0" text="Save" />
    <TableView fx:id="table" layoutX="14.0" layoutY="206.0" prefHeight="325.0" prefWidth="732.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="183.0" text="Name" fx:id="cName" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="163.0" text="Phone" fx:id="cPhone" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="381.0" text="Address" fx:id="cAddress" />
      </columns>
    </TableView>
  </children>
</AnchorPane>
